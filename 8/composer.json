{
    "name": "computerminds/composer-cache-warm",
    "description": "Warm up those composer caches",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "ComputerMinds",
            "email": "info@computerminds.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "composer/installers": "^1.0.21",
        "oomphinc/composer-installers-extender": "dev-master",
        "drupal-composer/drupal-scaffold": "^1.3.1",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "~8.1",
        "drush/drush": "~8.0",
        "drupal/add_to_head": "dev-8.x-1.x",
        "drupal/admin_toolbar": "^8.1.14",
        "drupal/config_readonly": "8.1.0-beta2",
        "drupal/console": "~0.10",
        "drupal/config_devel": "8.1.0-rc1",
        "drupal/csv_serialization": "8.1.0-alpha1",
        "drupal/devel": "8.1.x-dev",
        "drupal/features": "8.3.x-dev",
        "drupal/field_group": "8.1.0-rc4",
        "drupal/metatag": "8.1.0-beta7",
        "drupal/migrate_plus": "8.2.0-beta1",
        "drupal/migrate_tools": "8.2.x-dev",
        "drupal/migrate_upgrade": "8.2.0-beta1",
        "drupal/pathauto": "8.1.0-alpha2",
        "wikimedia/composer-merge-plugin": "^1.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-types": ["drupal-library"],
        "installer-paths": {
            "webroot/core": [
                "type:drupal-core"
            ],
            "webroot/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "webroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "webroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "webroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
