{
    "name": "computerminds/composer-cache-warm",
    "description": "Warm up those composer caches",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "ComputerMinds",
            "email": "info@computerminds.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
             "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.0.21",
        "computerminds/cm_security": "^8.1.1",
        "cweagans/composer-patches": "^1.6.0",
        "desandro/imagesloaded": "3.1.8",
        "desandro/masonry": "3.3.1",
        "drupal-composer/drupal-scaffold": "^1.3.1",
        "drupal/admin_toolbar": "^1.17",
        "drupal/cm_config_tools": "1.x-dev",
        "drupal/config_devel": "1.x-dev#92f735cae8e9fb328edca285340a5bfbf72028cc",
        "drupal/config_update": "^1.3",
        "drupal/console": "~0.10",
        "drupal/core": "^8",
        "drupal/devel": "^1@RC",
        "drupal/dropzonejs": "1.0-alpha2",
        "drupal/entity_browser": "^1.0",
        "drupal/entity_embed": "1.0-beta1",
        "drupal/field_group": "1.x-dev#68020f6f4b185c5989b70272ac32177d4c42b882",
        "drupal/file_browser": "^1.0",
        "drupal/file_entity": "2.x-dev#8accdd53fb9b87f3fe8462cfff020e4936d364e1",
        "drupal/metatag": "1.0-beta9",
        "drupal/paragraphs": "^1.0",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.0",
        "drush/drush": "^8.0",
        "enyo/dropzone": "4.2.0",
        "oomphinc/composer-installers-extender": "~1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-types": ["drupal-library"],
        "installer-paths": {
            "webroot/core": [
                "type:drupal-core"
            ],
            "webroot/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "webroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "webroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "webroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
